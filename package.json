  "version": "0.0.1",
  "devDependencies": {
    "@prettier/plugin-php": "^0.19.5",
    "autoprefixer": "^10.4.7",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "mini-css-extract-plugin": "^2.7.6",
