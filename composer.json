{
    "name": "brandcom/silverstripe-starter",
    "type": "silverstripe-recipe",
    "description": "Simplify the installation of a Silverstripe Dev Environment.",
    "require": {
        "php": "8.2.*",
        "dnadesign/silverstripe-elemental": "^5.1",
        "dnadesign/silverstripe-elemental-userforms": "^4.1",
        "jbennecker/silverstripe-analytics": "dev-ss5",
        "jbennecker/silverstripe-honeypotprotection": "dev-ss5",
        "jbennecker/silverstripe-webp": "dev-ss5",
        "silverstripe/googlesitemaps": "^3.0",
        "silverstripe/login-forms": "^5.1.0",
        "silverstripe/recipe-cms": "^5.1.0",
        "silverstripe/recipe-plugin": "^2.0.0",
        "silverstripe/redirectedurls": "^3.0.0",
        "silverstripe/vendor-plugin": "^2.0.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "extra": {
        "project-files-installed": [
            ".htaccess",
            "app/.htaccess",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "config": {
        "sort-packages": true,
        "process-timeout": 600,
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
